{
    "file_patterns": [
        "application/layouts/scripts/twbs3*/*.phtml",
        "application/views/**/*.phtml",
        "application/modules/admin/views/**/*.phtml"
    ],
    "patterns": [
        {
            "match": "$this->baseUrl('lib/js/hogan/hogan-3.0.2.min.js')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/hogan.js/3.0.2/hogan.min.js'"
        },
        {
            "match": "$this->baseUrl('lib/js/sprintfjs/sprintf.min.js')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/sprintf/1.0.1/sprintf.min.js'"
        },
        {
            "match": "$this->baseUrl('lib/js/morris/raphael-min.js')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.4/raphael-min.js'"
        },
        {
            "match": "$this->baseUrl('lib/js/morris/morris.min.js')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/morris.js/0.5.1/morris.min.js'"
        },
        {
            "match": "$this->baseUrl('lib/css/morris/morris.css')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/morris.js/0.5.1/morris.css'"
        },
        {
            "match": "$this->baseUrl('lib/js/ace-editor/ace.js')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/ace/1.1.9/ace.js'"
        },
        {
            "match": "$this->baseUrl('lib/js/ace-editor/mode-html.js')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/ace/1.1.9/mode-html.js'"
        },
        {
            "match": "$this->baseUrl('lib/js/bootstrap-switch/bootstrap-switch.min.js')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/bootstrap-switch/3.3.2/js/bootstrap-switch.min.js'"
        },
        {
            "match": "$this->baseUrl('lib/css/bootstrap-switch/bootstrap-switch.min.css')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/bootstrap-switch/3.3.2/css/bootstrap3/bootstrap-switch.min.css'"
        },
        {
            "match": "$this->baseUrl('lib/js/maskedinput/jquery.maskedinput.min.js')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/jquery.maskedinput/1.3.1/jquery.maskedinput.min.js'"
        },
        {
            "match": "$this->baseUrl('js/jquery.qtip.min.js')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/qtip2/2.2.1/jquery.qtip.min.js'"
        },
        {
            "match": "$this->baseUrl('css/jquery.qtip.min.css')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/qtip2/2.2.1/jquery.qtip.min.css'"
        },
        {
            "match": "$this->baseUrl('js/ckeditor/ckeditor.js')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/ckeditor/4.0.1/ckeditor.min.js'"
        },
        {
            "match": "$this->baseUrl('js/prettify.js')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/prettify/r298/prettify.min.js'"
        },
        {
            "match": "$this->baseUrl('css/prettify.css')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/prettify/r298/prettify.min.css'"
        },
        {
            "match": "$this->baseUrl('lib/js/data-tables/jquery.dataTables.min.js')",
            "replacement": "'//cdn.datatables.net/1.10.7/js/jquery.dataTables.min.js'"
        },
        {
            "match": "$this->baseUrl('lib/js/data-tables/dataTables.bootstrap.js')",
            "replacement": "'//cdn.datatables.net/plug-ins/1.10.7/integration/bootstrap/3/dataTables.bootstrap.js'"
        },
        {
            "match": "$this->baseUrl('lib/css/font-awesome/font-awesome.min.css')",
            "replacement": "'//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css'"
        },
        {
            "match": "$this->baseUrl('lib/js/jquery/jquery-1.10.2.min.js')",
            "replacement": "'//code.jquery.com/jquery-1.10.2.min.js'"
        },
        {
            "match": "$this->baseUrl('js/jquery-ui-1.9.2.custom.min.js')",
            "replacement": "'http://code.jquery.com/ui/1.9.2/jquery-ui.min.js'"
        },
        {
            "match": "$this->baseUrl('lib/css/bootstrap/default/bootstrap.min.css')",
            "replacement": "'//maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css'"
        },
        {
            "match": "$this->baseUrl('lib/js/parsley/parsley.min.js')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/parsley.js/2.0.5/parsley.min.js'"
        },
        {
            "match": "$this->baseUrl('lib/js/full-pagejs/jquery.fullPage.min.js')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/fullPage.js/2.4.8.1/jquery.fullPage.min.js'"
        },
        {
            "match": "$this->baseUrl('lib/css/full-pagejs/jquery.fullPage.css')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/fullPage.js/2.4.8.1/jquery.fullPage.min.css'"
        },
        {
            "match": "$this->baseUrl('lib/js/leaflet/leaflet.js')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/leaflet/0.7.3/leaflet.js'"
        },
        {
            "match": "$this->baseUrl('lib/css/leaflet/leaflet.css')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/leaflet/0.7.3/leaflet.css'"
        },
        {
            "match": "$this->baseUrl('js/moment-with-langs.min.js')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.6.0/moment-with-langs.min.js'"
        },
        {
            "match": "$this->baseUrl('lib/js/sprintfjs/sprintf.min.js')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/sprintf/1.0.1/sprintf.min.js'"
        },
        {
            "match": "$this->baseUrl('lib/css/animate/animate.min.css')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.3.0/animate.min.css'"
        },
        {
            "match": "$this->baseUrl('lib/js/select2/select2.min.js')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/select2/3.5.1/select2.min.js'"
        },
        {
            "match": "$this->baseUrl('lib/css/select2/select2.css')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/select2/3.5.1/select2.min.css'"
        },
        {
            "match": "$this->baseUrl('lib/css/select2/select2-bootstrap.css')",
            "replacement": "'https://cdnjs.cloudflare.com/ajax/libs/select2-bootstrap-css/1.4.1/select2-bootstrap.min.css'"
        },
        {
            "match": "$this->baseUrl('lib/js/alertifyjs/alertify.min.js')",
            "replacement": "'//cdn.jsdelivr.net/alertifyjs/1.4.1/alertify.min.js'"
        },
        {
            "match": "$this->baseUrl('lib/css/alertifyjs/alertify.min.css')",
            "replacement": "'//cdn.jsdelivr.net/alertifyjs/1.4.1/css/alertify.min.css'"
        },
        {
            "match": "$this->baseUrl('lib/css/alertifyjs/themes/bootstrap.min.css')",
            "replacement": "'//cdn.jsdelivr.net/alertifyjs/1.4.1/css/themes/bootstrap.min.css'"
        },
        {
            "match": "$this->baseUrl('lib/js/vue/vue.js')",
            "replacement": "'//unpkg.com/vue@2.4.2/dist/vue.min.js'"
        }
    ]
}
